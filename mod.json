{
	"geode": "4.9.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "timestepyt.tournamentmod",
	"name": "TournamentMod",
	"version": "v1.0.0",
	"developer": "TimeStepYT",
	"description": "Client for the tournament",
	"dependencies": {
		"geode.node-ids": {
			"version": "1.21.0",
			"importance": "required"
		}
	},
	"resources": {
		"sprites": ["resources/*.png"]
	},
	"settings": {
		"server-url": {
			"type": "string",
			"default": "localhost",
			"name": "URL",
			"match": "[A-Za-z0-9\\.]+",
			"filter": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789.-_"
		},
		"server-port": {
			"type": "int",
			"default": 19992,
			"name": "Port"
		}
	}
}